# AI Configuration
# OpenAI API Key (required for OpenAI provider)
OPENAI_API_KEY=your_openai_api_key_here

# Alternative AI Providers (optional)
# Anthropic API Key (for Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI API Key (for Gemini)
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Azure OpenAI (optional)
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint_here

# AI Provider Selection (default: openai)
AI_PROVIDER=openai

# Git Configuration for Commit Tracking
# Default Git user name for commits (fallback if user doesn't provide)
GIT_USER_NAME=OpenMed System

# Default Git user email for commits (fallback if user doesn't provide)
GIT_USER_EMAIL=system@openmed.formidable.care

